{
    "name": "anonyzm/firestore-php",
    "description": "Firestore SDK for PHP without gRPC",
    "keywords": ["firestore", "google", "sdk", "api", "database"],
    "homepage": "https://github.com/anonyzm/firestore-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonid",
            "email": "bazoonster@gmail.com"
        }
    ],
    "support": {
        "docs": "https://github.com/anonyzm/firestore-php/",
        "issues": "https://github.com/anonyzm/firestore-php/issues",
        "source": "https://github.com/anonyzm/firestore-php"
    },
    "scripts": {
        "test": "phpunit"
    },
    "require": {
        "kreait/firebase-php": "^4.11",
        "google/protobuf": "v3.6.0.1"
    },
    "require-dev": {
        "morrislaptop/var-dumper-with-context": "^0.2.0",
        "symfony/var-dumper": "^4.0",
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Morrislaptop\\Firestore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morrislaptop\\Firestore\\Tests\\": "tests",
            "Kreait\\Firebase\\Tests\\": "vendor/kreait/firebase-php/tests"
        }
    }
}
